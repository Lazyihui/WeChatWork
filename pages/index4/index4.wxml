<view class="container">
  <!-- 一层 -->
  <view class="table">
    <view wx:for="{{table}}" wx:key="index" class="row">
      <view wx:for="{{item}}" wx:key="index" class="cell">{{item}}</view>
    </view>
  </view>
</view>
<button class="nextButton" bind:tap="goToNextPage">下一个</button>
<view class="container">
  <view class="table">
    <view class="row">
      <view class="cell">1 * 1 = 1</view>
    </view>
    <view class="row">
      <view class="cell">1 * 2 = 2</view>
      <view class="cell">2 * 2 = 4</view>
    </view>
    <view class="row">
      <view class="cell">1 * 3 = 3</view>
      <view class="cell">2 * 3 = 6</view>
      <view class="cell">3 * 3 = 9</view>
    </view>
  </view>
</view>